<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Mobile_Number_Range</fullName>
    <active>true</active>
    <errorConditionFormula>LEN(Mobile__c) &lt;10</errorConditionFormula>
    <errorMessage>Mobile number should be 10 digits</errorMessage>
</ValidationRule>
